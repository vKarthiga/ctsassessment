public class DataTypeDemo {
    public static void main(String[] args) {
        int age = 25;
        float height = 5.9f;
        double salary = 55000.75;
        char grade = 'A';
        boolean isEmployed = true;

        System.out.println("Integer (int) value: " + age);
        System.out.println("Float (float) value: " + height);
        System.out.println("Double (double) value: " + salary);
        System.out.println("Character (char) value: " + grade);
        System.out.println("Boolean (boolean) value: " + isEmployed);
    }
}
